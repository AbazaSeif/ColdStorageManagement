<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0"
	xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2"
	fx:controller="com.mg.controller.RegistrationController">
	<children>
		<BorderPane prefHeight="400.0" prefWidth="600.0"
			AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
			AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
			<bottom>
				<HBox alignment="CENTER" BorderPane.alignment="CENTER" />
			</bottom>
			<center>
				<HBox alignment="CENTER" BorderPane.alignment="CENTER">
					<children>
						<AnchorPane maxHeight="296.0" maxWidth="1000.0"
							minHeight="200.0" minWidth="350.0" prefHeight="296.0" prefWidth="350.0">
							<children>
								<TextField fx:id="tfUserName" layoutX="44.0" layoutY="69.0"
									prefHeight="31.0" prefWidth="270.0" promptText="Enter User Name">
									<tooltip>
										<Tooltip text="User Name will need to Login" />
									</tooltip>
								</TextField>
								<TextField fx:id="tfFullName" layoutX="44.0" layoutY="115.0"
									prefHeight="31.0" prefWidth="270.0" promptText="Enter Full Name" />
								<PasswordField fx:id="pfUserPassword" layoutX="44.0"
									layoutY="163.0" prefHeight="31.0" prefWidth="269.0" promptText="Type Password" />
								<PasswordField fx:id="pfReUserPassword" layoutX="44.0"
									layoutY="210.0" onKeyReleased="#pfKeyTyped" prefHeight="31.0"
									prefWidth="269.0" promptText="Re-Type Password" />
								<Button fx:id="btnSignUp" layoutX="123.0" layoutY="261.0"
									mnemonicParsing="false" onAction="#btnRegistration" text="Signup" />
								<Text layoutX="102.0" layoutY="40.0" strokeType="OUTSIDE"
									strokeWidth="0.0" text="Registration">
									<font>
										<Font size="24.0" />
									</font>
								</Text>
							</children>
						</AnchorPane>
					</children>
				</HBox>
			</center>
			<top>
				<HBox alignment="CENTER" BorderPane.alignment="CENTER" />
			</top>
		</BorderPane>
	</children>
</AnchorPane>
