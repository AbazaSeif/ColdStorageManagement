<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.mg.csms.beans.InwardInventory" table="InwardInventory">
		<id name="coldId" type="integer" column="COLD_ID">
			<generator class="increment" />
		</id>
		<property name="date" column="IN_DATE"/>
		<property name="hsnCode" column="HSN_CODE" />
		<property name="itemName" column="ITEM_NAME" />
		<property name="partyId" column="PARTY_ID" />
		<property name="lotNo" column="LOT_NO" />
		<property name="qty" column="QUANTITY" />
		<property name="bardana" column="BARDANA" />
		<property name="gadiNo" column="GADI_NO" />

	</class>
</hibernate-mapping>