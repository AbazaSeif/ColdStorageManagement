<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.mg.csms.beans.ColdStorage" table="COLD_DETAILS">
		<id name="coldId" type="integer" column="COLD_ID">
			<generator class="increment" />
		</id>
		<property name="date" column="ENTRY_DATE" />
		<property name="coldName" column="COLD_NAME" />
		<property name="address" column="COLD_ADDRESS" />
		<property name="phoneNo" column="PHONE_NUMBER" />

	</class>
</hibernate-mapping>